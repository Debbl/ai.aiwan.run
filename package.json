{
  "name": "ai.aiwan.run",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "description": "starter next app",
  "author": "Brendan Dash <me@aiwna.run> (https://github.com/Debbl/)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/ai.aiwan.run",
  "repository": {
    "type": "git",
    "url": "https://github.com/Debbl/ai.aiwan.run.git"
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "db:push": "turbo db:push",
    "db:gen": "turbo db:gen",
    "db:migrate": "turbo db:migrate",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "typecheck": "turbo typecheck",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@debbl/utils": "^0.2.2",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/serverless": "^3.52.1",
    "@workspace/env": "workspace:*",
    "@workspace/server": "workspace:*",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@debbl/eslint-config": "^3.11.0",
    "@opennextjs/aws": "^3.6.1",
    "@opennextjs/cloudflare": "^1.0.2",
    "@types/node": "^22.15.17",
    "eslint": "^9.26.0",
    "lint-staged": "^15.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "simple-git-hooks": "^2.13.0",
    "turbo": "^2.5.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "wrangler": "^4.14.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
