{
  "name": "ai.aiwan.run",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "description": "starter next app",
  "author": "Brendan Dash <me@aiwna.run> (https://github.com/Debbl/)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/ai.aiwan.run",
  "repository": {
    "type": "git",
    "url": "https://github.com/Debbl/ai.aiwan.run.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:cf": "opennextjs-cloudflare build",
    "db:push": "drizzle-kit push",
    "db:push:prod": "NODE_ENV=production drizzle-kit push",
    "db:gen": "NODE_ENV=development drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --keep-vars",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "auth:gen": "npx @better-auth/cli generate --output ./src/server/db/schema/internal/auth.ts",
    "db:migrate:dev": "wrangler d1 migrations apply ai-aiwan-run --local",
    "db:migrate:prod": "wrangler d1 migrations apply ai-aiwan-run --remote",
    "i18n:extract": "lingui extract",
    "typecheck": "tsc --noEmit",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "beta",
    "@ai-sdk/openai": "beta",
    "@ai-sdk/react": "beta",
    "@debbl/ahooks": "^0.4.1",
    "@debbl/utils": "^0.2.2",
    "@huggingface/transformers": "3.5.2",
    "@lingui/core": "^5.4.0",
    "@lingui/react": "^5.4.0",
    "@orpc/client": "^1.7.9",
    "@orpc/server": "^1.7.9",
    "@orpc/tanstack-query": "^1.7.9",
    "@tailwindcss/postcss": "^4.1.11",
    "@tanstack/react-query": "^5.84.1",
    "ai": "beta",
    "better-auth": "^1.3.4",
    "birpc": "^2.5.0",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "es-toolkit": "^1.39.8",
    "jotai": "^2.12.5",
    "lucide-react": "^0.533.0",
    "motion": "12.18.1",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "next-view-transitions": "^0.3.4",
    "nuqs": "^2.4.3",
    "radix-ui": "^1.4.2",
    "react": "^19.1.1",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-resizable-panels": "^3.0.4",
    "serwist": "^9.1.1",
    "sonner": "^2.0.7",
    "tinycolor2": "^1.6.0",
    "ts-pattern": "^5.8.0",
    "twl": "^0.0.0",
    "unplugin-auto-import": "^19.3.0",
    "use-is-match-media": "^0.0.2",
    "use-transformers": "^0.0.10",
    "zod": "^4.0.14",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.4",
    "@debbl/eslint-config": "^3.12.0",
    "@lingui/cli": "^5.4.0",
    "@lingui/loader": "^5.4.0",
    "@lingui/swc-plugin": "^5.5.2",
    "@next/bundle-analyzer": "15.5.0",
    "@next/env": "15.5.0",
    "@opennextjs/cloudflare": "^1.6.3",
    "@serwist/next": "^9.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/tinycolor2": "^1.4.6",
    "better-sqlite3": "^12.2.0",
    "eslint": "^9.32.0",
    "jiti": "^2.5.1",
    "lint-staged": "^16.1.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "sharp": "^0.34.3",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4.1.11",
    "type-fest": "^4.41.0",
    "typescript": "^5.9.2",
    "wrangler": "^4.27.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
